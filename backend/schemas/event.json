{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SupportEvent",
  "type": "object",
  "properties": {
    "id": { "type": "string", "description": "Unique event identifier" },
    "type": { "type": "string", "description": "Type of event" },
    "timestamp": { "type": "string", "format": "date-time", "description": "Event time" },
    "region_id": { "type": "integer", "description": "Associated region identifier" },
    "payload": { "type": "object", "description": "Event-specific data" }
  },
  "required": ["id", "type", "timestamp", "region_id"],
  "additionalProperties": false
}
